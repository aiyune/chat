{
    "branches": ["main"],
    "plugins": [
        [
            "@semantic-release/commit-analyzer",
            {
                "releaseRules": [
                    { "type": "build", "release": "patch" },
                    { "type": "ci", "release": "patch" },
                    { "type": "chore", "release": "patch" },
                    { "type": "docs", "release": "patch" },
                    { "type": "refactor", "release": "patch" },
                    { "type": "style", "release": "patch" },
                    { "type": "test", "release": "patch" }
                ]
            }
        ],
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/changelog",
            {
                "changelogTitle": "# Changelog @aiyune/chat-aigc\n\n ðŸ‘‹Hello! This is where all important project changes are recorded. \nðŸ“ŒThe format is based on [Keep a Changelog](https://keepachangelog.com/en/1.0.0/), and this project adheres to [Semantic Versioning](https://semver.org/spec/v2.0.0.html)."
            }
        ],
        "@semantic-release/npm",
        [
                "@semantic-release/git",
                {
                    "assets": ["CHANGELOG.md", "package.json"],
                    "message": "ðŸ”–chore(release): Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
                }
        ],
        "@semantic-release/github"
    ]
}